      <archive extract="cmake-2.8.7-Linux-i386" href="http://www.cmake.org/files/v2.8/cmake-2.8.7-Linux-i386.tar.gz" size="16999959" type="application/x-compressed-tar"/>
    </implementation>
    <implementation arch="Darwin-*" id="sha1new=ca8b84c2ac99486677e144017e380191697e3143" main="CMake 2.8-7.app/Contents/bin/cmake">
      <archive extract="cmake-2.8.7-Darwin-universal" href="http://www.cmake.org/files/v2.8/cmake-2.8.7-Darwin-universal.tar.gz" size="29756918" type="application/x-compressed-tar"/>
