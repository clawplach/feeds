<?xml version='1.0' ?>
<cmake-project>
  <name>BoostMath</name>
  <find-package>
    <arg>Boost</arg>
    <arg>COMPONENTS</arg>
    <arg>algorithm</arg>
    <arg>array</arg>
    <arg>bind</arg>
    <arg>concept_check</arg>
    <arg>config</arg>
    <arg>conversion</arg>
    <arg>core</arg>
    <arg>exception</arg>
    <arg>format</arg>
    <arg>function</arg>
    <arg>fusion</arg>
    <arg>integer</arg>
    <arg>mpl</arg>
    <arg>numeric</arg>
    <arg>random</arg>
    <arg>regex</arg>
    <arg>smart_ptr</arg>
    <arg>spirit</arg>
    <arg>static_assert</arg>
    <arg>test</arg>
    <arg>timer</arg>
    <arg>tr1</arg>
    <arg>tuple</arg>
    <arg>type_traits</arg>
    <arg>utility</arg>
    <arg>NO_MODULE</arg>
  </find-package>
  <depends>
    <dependency>BoostAlgorithm</dependency>
    <dependency>BoostArray</dependency>
    <dependency>BoostBind</dependency>
    <dependency>BoostConceptCheck</dependency>
    <dependency>BoostConfig</dependency>
