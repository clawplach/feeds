    </implementation>
    <command name="compile">
      <runner interface="https://raw.github.com/ryppl/feeds/gh-pages/ryppl/ryppl.xml">
        <arg>writable-directory-overlay</arg>
        <arg>${SRCDIR}</arg>
        <arg>${DISTDIR}</arg>
      </runner>
      <requires interface="https://raw.github.com/ryppl/feeds/gh-pages/boost/chrono-date_time-filesystem-graph-graph_parallel-iostreams-mpi-math-python-random-regex-serialization-system-test-thread-timer-preinstall.xml">
