<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="xpressive-dev.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface" xmlns:compile="http://zero-install.sourceforge.net/2006/namespaces/0compile" xmlns:dc="http://purl.org/dc/elements/1.1/">
  <name>Boost.Xpressive (development files)</name>
  <icon href="http://svn.boost.org/svn/boost/website/public_html/live/gfx/boost-dark-trans.png" type="image/png"/>
  <summary>A C++ library from Boost.org</summary>
  <homepage>http://www.boost.org/libs/xpressive/</homepage>
  <dc:author>Eric Niebler</dc:author>
  <description>Regular expressions that can be written as strings
    or as expression templates, and which can refer to each other
    and themselves recursively with the power of context-free
    grammars.</description>
  <category type="http://boost.org/lib-categories">String</category>
  <group license="OSI Approved :: Boost Software License 1.0 (BSL-1.0)">
    <implementation arch="*-src" id="4180b08b-f94e-46b2-b5a2-899cd6517392" released="2012-05-01" stability="testing" version="1.49-post-201205020104"/>
    <command name="compile">
      <runner interface="http://ryppl.github.com/feeds/ryppl/0cmake.xml">
        <version not-before="0.8-pre-201205011504"/>
        <arg>headers</arg>
      </runner>
      <requires interface="http://ryppl.github.com/feeds/boost/xpressive-src.xml">
        <environment insert="." mode="replace" name="SRCDIR"/>
      </requires>
      <requires interface="http://ryppl.github.com/feeds/boost/CMakeLists.xml">
        <environment insert="xpressive" mode="replace" name="BOOST_CMAKELISTS_OVERLAY"/>
      </requires>
      <compile:implementation arch="*-*"/>
    </command>
  </group>
</interface>
