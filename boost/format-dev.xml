    </implementation>
    <command name="compile">
      <runner interface="http://ryppl.github.com/feeds/ryppl/ryppl.xml">
        <arg>0install-cmake</arg>
        <arg>--overlay=${BOOST_CMAKELISTS}</arg>
        <arg>dev</arg>
      </runner>
      <requires interface="http://ryppl.github.com/feeds/boost/CMakeLists.xml">
        <environment insert="format" mode="replace" name="BOOST_CMAKELISTS"/>
      </requires>
      <compile:implementation arch="*-*"/>
    </command>
  </group>
</interface>
