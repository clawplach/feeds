<?xml version='1.0' ?>
<cmake-project>
  <name>BoostAny</name>
  <find-package>
    <arg>Boost</arg>
    <arg>COMPONENTS</arg>
    <arg>config</arg>
    <arg>NO_MODULE</arg>
  </find-package>
  <depends>
    <dependency>BoostConfig</dependency>
    <dependency>BoostException</dependency>
    <dependency>BoostStaticAssert</dependency>
    <dependency>BoostTypeTraits</dependency>
  </depends>
  <include-directories>
    <directory>include</directory>
  </include-directories>
</cmake-project>
