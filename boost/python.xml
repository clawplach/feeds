      <manifest-digest sha1new="da39a3ee5e6b4b0d3255bfef95601890afd80709"/>
    </implementation>
    <command name="compile">
      <runner interface="http://ryppl.github.com/feeds/ryppl/ryppl.xml">
        <arg>writable-directory-overlay</arg>
        <arg>${SRCDIR}</arg>
        <arg>${DISTDIR}</arg>
      </runner>
      <requires interface="http://ryppl.github.com/feeds/boost/python-preinstall.xml">
        <environment insert="bin" mode="replace" name="SRCDIR"/>
      </requires>
    </command>
  </group>
</interface>
