    <dependency>BoostNumeric</dependency>
    <dependency>BoostSmartPtr</dependency>
    <dependency>BoostSpirit</dependency>
    <dependency>BoostStaticAssert</dependency>
    <dependency>BoostTR1</dependency>
    <dependency>BoostTuple</dependency>
    <dependency>BoostTypeTraits</dependency>
    <dependency>BoostUtility</dependency>
  </depends>
  <include-directories>
    <directory>include</directory>
  </include-directories>
  <libraries>
    <library>boost_math_tr1</library>
    <library>boost_math_tr1f</library>
    <library>boost_math_tr1l</library>
    <library>boost_math_c99</library>
    <library>boost_math_c99f</library>
    <library>boost_math_c99l</library>
  </libraries>
</cmake-project>
